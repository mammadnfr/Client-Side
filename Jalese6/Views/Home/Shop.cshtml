@{
    ViewData["Title"] = "Shop";
}
<table class="table table-responsive">
    <tr>
        <th>Id</th>
        <th>Name</th>
        <th>Family</th>
        <th>Gender</th>
        <th>BirthDay</th>
    </tr>
    @{
        foreach (var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Name</td>
                <td>@item.Family</td>
                <td>@item.Gender</td>
                <td>@item.BirthDay</td>
            </tr>
        }
    }
</table>
